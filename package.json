{
  "name": "webpack-rails-demo",
  "version": "0.0.0",
  "description": "Show rails using webpack as its asset manager.",
  "scripts": {
    "build": "webpack --config webpack/config.js --progress --profile --colors",
    "watch": "webpack -w --config webpack/config.js --progress --profile --colors",
    "postinstall": "npm run build"
  },
  "author": "Derek Dahmer",
  "dependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.5.8",
    "babel-loader": "^5.1.4",
    "jquery": "^2.1.4",
    "webpack": "^1.9.11"
  }
}
